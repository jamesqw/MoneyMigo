<div class="page-header">
  <h1>Import Statement <small><%- accountName %></small></h1>
</div>

<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="import_file" class="col-md-2 control-label">File</label>
    <div class="col-md-5">
      <input type="file" id="import_file" name="file" />
    </div>
    <div class="errors col-md-5"></div>
  </div>
  <div class="form-group">
    <div class="col-md-offset-2 col-md-10">
      <button type="submit" data-loading-text="Importing..." class="save btn btn-default" title="Import">Import</button>
      <a class="btn btn-link" href="<%= DotLedger.path.showAccount({id: accountId}) %>" title="Cancel">Cancel</a>
    </div>
  </div>
</form>
